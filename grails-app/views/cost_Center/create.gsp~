<script>
	$(function(){
	$("#slider-range-min1").slider({
	range:"min",
	value:50,
	min:0,
	max:100,
	slide:function(event,ui){
	$("#amount1").val(ui.value+"00");
	$(".a,.b,.c,.d").width(ui.value+"%");
	}
	});
	$(".ui-slider-handle").text("<>");
	$("#amount1").val($("#slider-range-min1").slider("value")+"00");
	});
	</script>

